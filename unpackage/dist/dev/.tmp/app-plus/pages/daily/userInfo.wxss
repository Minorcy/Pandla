    text-shadow: none;
  }
}
/* Code blocks */
._pre[class*="language-"] {
  padding: 1em;
  margin: .5em 0;
  overflow: auto;
}
:not(._pre)>._code[class*="language-"],
._pre[class*="language-"] {
  background: #f5f2f0;
}
/* Inline code */
:not(._pre)>._code[class*="language-"] {
  padding: .1em;
  border-radius: .3em;
  white-space: normal;
}
