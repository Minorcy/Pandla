<view class="fans-page"><view class="status_bar"></view><view class="tabbar"><view data-event-opts="{{[['tap',[['goBack']]]]}}" class="back" bindtap="__e"><image src="../../static/img/user/follow/back.svg" mode></image></view><view class="uni-swiper-tab" id="tab-bar" scroll-x scroll-left="{{scrollLeft}}"><block wx:for="{{tabBars}}" wx:for-item="tab" wx:for-index="index" wx:key="id"><view class="{{['swiper-tab-list',tabIndex==index?'active':'']}}" id="{{tab.id}}" data-current="{{index}}" data-event-opts="{{[['tap',[['tapTab',['$event']]]]]}}" bindtap="__e">{{tab.name}}</view></block></view></view><swiper class="swiper-box" current="{{tabIndex}}" duration="{{300}}" data-event-opts="{{[['change',[['changeTab',['$event']]]]]}}" bindchange="__e"><swiper-item><scroll-view class="folllow-list" scroll-y><block wx:for="{{followList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view class="list-item"><image class="item-img" src="{{item.portrait}}" mode="aspectFill" data-event-opts="{{[['tap',[['userInfo',['$0'],[[['followList','id',item.id,'id']]]]]]]}}" bindtap="__e"></image><view class="list-item-right"><view class="item-userInfo"><text class="name">{{item.name}}</text><text class="circles">{{item.age+"/"+item.stature+"/"+item.weight+"/"+item.status}}</text><text class="address">{{item.site}}</text></view><view class="item-btn"><image src="../../static/img/user/follow/cancel.svg" mode data-event-opts="{{[['tap',[['unFollow',['$0',index],[[['followList','id',item.id,'id']]]]]]]}}" bindtap="__e"></image></view></view></view></block></scroll-view></swiper-item><swiper-item><scroll-view class="folllow-list" scroll-y><block wx:for="{{fnasList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view class="list-item"><image class="item-img" src="{{item.portrait}}" mode="aspectFill" data-event-opts="{{[['tap',[['userInfo',['$0'],[[['fnasList','id',item.id,'id']]]]]]]}}" bindtap="__e"></image><view class="list-item-right"><view class="item-userInfo"><text class="name">{{item.name}}</text><text class="circles">{{item.age+"/"+item.stature+"/"+item.weight+"/"+item.status}}</text><text class="address">{{item.site}}</text></view><view class="item-btn"><image src="../../static/img/user/follow/follow.svg" mode data-event-opts="{{[['tap',[['follow',['$0',index],[[['fnasList','id',item.id,'id']]]]]]]}}" bindtap="__e"></image></view></view></view></block></scroll-view></swiper-item><swiper-item><scroll-view class="folllow-list" scroll-y><block wx:for="{{friendList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view class="list-item"><image class="item-img" src="{{item.portrait}}" mode="aspectFill" data-event-opts="{{[['tap',[['userInfo',['$0'],[[['friendList','id',item.id,'id']]]]]]]}}" bindtap="__e"></image><view class="list-item-right"><view class="item-userInfo"><text class="name">{{item.name}}</text><text class="circles">{{item.age+"/"+item.stature+"/"+item.weight+"/"+item.status}}</text><text class="address">{{item.site}}</text></view><view class="item-btn"><image src="../../static/img/user/follow/mutual.svg" mode data-event-opts="{{[['tap',[['unMutual',['$0',index],[[['friendList','id',item.id,'id']]]]]]]}}" bindtap="__e"></image></view></view></view></block></scroll-view></swiper-item></swiper></view>