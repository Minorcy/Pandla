<view class="album-page"><view class="list-box"><block wx:for="{{dynInfo}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{[(item.active)?'active':'']}}" data-index="{{index}}" data-event-opts="{{[['tap',[['previewPhoto',[index]]]]]}}" bindtap="__e"><image src="{{item.images}}" mode="aspectFill" lazy-load="true"></image><image class="delete" src="../../static/img/user/delete.svg" mode data-event-opts="{{[['tap',[['deleteDyn',['$0',index],[[['dynInfo','',index,'id']]]]]]]}}" catchtap="__e"></image><block wx:if="{{item.content==''}}"><view>{{"第"+(index+1)+"图片"}}</view></block><block wx:if="{{item.content!=''}}"><view>{{item.content}}</view></block></view></block></view><view class="load">{{loadTxt}}</view></view>